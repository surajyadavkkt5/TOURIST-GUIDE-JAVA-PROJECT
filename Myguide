		case 23:System.out.println("****************************");
			System.out.println("WELCOME TO TRIPURA(:");
			System.out.println("****************************");
			d.tripur();
		break;
		case 24: System.out.println("****************************");
			System.out.println("WELCOME TO UTTRAKHAND(:");
			System.out.println("****************************");
			d.uttrakhand();
		break;
		case 25: System.out.println("****************************");
			System.out.println("WELCOME TO UTTAR PRADESH(:");
			System.out.println("****************************");
			d.uttarPradesh();
		break;
		case 26: System.out.println("****************************");
			System.out.println("WELCOME TO WEST BENGAL(:");
			System.out.println("****************************");
			d.westBengal();
		break;
		case 27: System.out.println("****************************");
			System.out.println("WELCOME TO  ANDAMAN AND NICOBAR ISLANDS(:");
			System.out.println("****************************");
			d.aani();
		break;
		case 28: System.out.println("****************************");
			System.out.println("WELCOME TO CHANDIGARH(:");
			System.out.println("****************************");
			d.chandigarh();
		break;
		case 29: System.out.println("****************************");
			System.out.println("WELCOME TO DELHI(:");
			System.out.println("****************************");
			d.delhi();
		break;
		case 30:System.out.println("****************************");
			System.out.println("WELCOME TO JAMMU & KASHMIR(:");
			System.out.println("****************************");
			d.j$k();
		break;
		case 31: System.out.println("****************************");
			System.out.println("WELCOME TO LADHAK(:");
			System.out.println("****************************");
			d.ladhak();
		break;
		
		default : System.out.println(" Please Choose Valid Number");
		           states();
		}
	}
	}
// extended class
	class Destination extends States{
		
		public void andraPradesh() {
			System.out.println("==================================================================");
			System.out.println("------Choose your favourite place to visit in Andra Pradesh:------");
			System.out.println("==================================================================");
			System.out.println(" ");
			System.out.println("1. Visakhapatnam\n2. Tirupati\n3. Vijayawada\n4. Nellore\n5. Chittoor"
					+ "\n6. Anantapur\n7. Kurnool\n8. Kadapa\n9. Srikalahasti\n10. Ongole"
					+ "\n11. Puttaparthi\n12. Rajahmundry\n13. Amalapuram\n14. Srikakulam\n15. Kakinada");
			System.out.println(" ");
			States s=new Destination();// Up casting
			Scanner sc = new Scanner(System.in);
			System.out.println("Enter 'B' to return back");
			char m=sc.next().charAt(0);
			switch (m) {
			case 'b': states();
			 break;
			case 'B': states();
			break;
			 default : System.out.println(" invalid Character");
			 andraPradesh();
			}
		}

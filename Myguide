			System.out.println("WELCOME TO HARYANA(:");
			System.out.println("****************************");
			d.haryana();
		break;
		case 9: System.out.println("****************************");
			System.out.println("WELCOME TO HIMACHAL PRADESH(:");
			System.out.println("****************************");
			d.himachalpradesh();
		break;
		case 10: System.out.println("****************************");
			System.out.println("WELCOME TO JHARKHAND(:");
			System.out.println("****************************");
			d.jharkhand();
		break;
		case 11: System.out.println("****************************");
			System.out.println("WELCOME TO KARNATKA(:");
			System.out.println("****************************");
			d.karnatka();
		break;

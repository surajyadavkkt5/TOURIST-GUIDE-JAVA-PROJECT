
import java.util.*;

// driver class
public class Myguide {
	// Main method
	public static void main(String[] args) {
	loginPage();
	Favourite f=new Favourite();
	f.fav();
		
	}
	// static block
		static{
			System.out.println("========================================");
			System.out.println("-------Welcome to MyGuide App(:---------");
			System.out.println("========================================");
		}
	public static void loginPage() // login page method
	{ 
		Scanner sc = new Scanner(System.in);
		System.out.println("Register your self : press [1] ");
		System.out.println("Login yourself : press [2] ");
		System.out.println(" ");
		System.out.print("Choose number: ");
		int n = sc.nextInt();
		
	      switch(n) {
		case 1 : 
			System.out.println("==============================");
			System.out.println("-----Enter your details[]-----");
		     System.out.println("============================= ");
		     Scanner sc1 = new Scanner(System.in);
		        System.out.print("Enter name: ");
     	         String name = sc1.nextLine();
     	        System.out.print("Enter Username: ");
    	         String uname = sc1.nextLine();
		           System.out.print("Set passsword: ");
		            String pass = sc1.nextLine();
		            System.out.print("Enter mobile number: ");
		           String number = sc1.nextLine();
		         System.out.print("Enter Email ID: ");
		        String email = sc1.nextLine();
		        System.out.println(" ");
		        System.out.println("======================== ");
		      System.out.print("Details submitted successfully (:\n");
		      System.out.println(" ");		     
		      System.out.println("Name: "+name);
		      System.out.println("Username: "+uname);
		      System.out.println("Password: "+pass);
		      System.out.println("Mobile Number: "+number);
		      System.out.println("Email-id: "+email);
		      System.out.println("========================= ");
		      login();
		      	break;
		
		case 2 : System.out.println("Login yourself[]");
		System.out.println(" ");
		String ed1 = sc.nextLine();
		 System.out.print("Enter Username: ");
	         String name1 = sc.nextLine();
           System.out.print("Enter passsword: ");
            String pass1 = sc.nextLine();
            System.out.println(" ");
            System.out.println(" Login Successfully (:");
            System.out.println(" ");
		break;
		
		default : System.out.println("Invalid Data");
		}
	}
	public static void login()
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Login yourself:");
		System.out.println(" ");
		//String ed1 = sc.nextLine();
		 System.out.print("Enter Username: ");
	         String name1 = sc.nextLine();
           System.out.print("Enter passsword: ");
            String pass1 = sc.nextLine();
            System.out.println(" ");
            System.out.println(" Login Successfully (:");
	}
}
class Favourite{
	public void fav() {
		System.out.println("***********************************************************");
		System.out.println("------------Choose your Favourite Temple to visit:---------");
		System.out.println("************************************************************");
System.out.println(" ");
		System.out.println("************************************************************");
		System.out.println("------------Choose your Favourite Beaches to visit:---------");
		System.out.println("************************************************************");
System.out.println(" ");
		System.out.println("**************************************************************************");
		System.out.println("------------Choose your Favourite Honeymoon destinations to visit:---------");
		System.out.println("**************************************************************************");
System.out.println(" ");
			System.out.println("***********************************************************");
			System.out.println("------------Choose your Favourite States to visit:---------");
			System.out.println("***********************************************************");
			System.out.println("=======================================");
			System.out.println("-----Enter number to choose States-----");
			System.out.println("=======================================");
			System.out.println(" ");
			System.out.println(" Press 1 To Visit Temples(:\n Press 2 To Visit Beaches(:\n Press 3 To Visit Honeymoon destinations(:\n Press 4 To Visit States(:");
			Scanner sc = new Scanner(System.in);
			int n=sc.nextInt();
		
			switch (n) {
			
			case 1: Temples ref=new Temples();
			        ref.temple();
			break;
			case 2:Temples ref1=new Temples();
	               ref1.beaches();
				break;
			case 3: Temples ref2=new Temples();
                    ref2.honeymoon();
				break;
			case 4: States ref4 = new States();
				    ref4.states();
				break;
			default : System.out.println(" Please Choose Valid Number");
			}
	}
	
}

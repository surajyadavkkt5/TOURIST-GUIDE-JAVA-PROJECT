	public void honeymoon() {
		System.out.println("============================================================================");
		System.out.println("------------Choose your Favourite Honeymoon destinations to visit(:---------");
		System.out.println("============================================================================");
		System.out.println(" ");
		System.out.println("1. Shimla\n2. Manali\n3. Dalhousie\n4. Auli\n5. Nainital"
				+ "\n6. Mussoorie)\n7. Darjeeling\n8. Andaman\n9. Gangtok\n10. Goa"
				+ "\n11. Udaipur\n12. Munnar\n13. Kumarakom\n14. Alleppey\n15. Ooty");
		System.out.println(" ");
		System.out.println("=========================================================");
		System.out.println("-----Enter number to choose Honeymoon destinations(:-----");
		System.out.println("=========================================================");
		System.out.println(" ");
		Scanner sc=new Scanner(System.in);
	int n=sc.nextInt();
		switch (n) {
		
		case 1:  System.out.println("----------------------\n***-SHIMLA-***\n----------------------\n ");
		System.out.println("***What To Eat***");
		System.out.println(" ");
		System.out.println("1. Prem Lahori Vaishno Dhaba:\n$ INR 500 For Two\nFood: Indian\nTime: 7Am-11PM");

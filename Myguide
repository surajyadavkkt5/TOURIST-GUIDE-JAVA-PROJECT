class States extends Favourite {
	
	public  void states() {
		System.out.println("===========================================================");
		System.out.println("------------Choose your Favourite States to visit:---------");
		System.out.println("===========================================================");
		System.out.println("1. ANDRA PRADESH\n2. ARUNACHAL PRADESH\n3. ASSAM\n4. BIHAR\n5. CHATTISGARH\n6. GOA\n7. GUJRAT\n"
				+ "8. HARYANA\n9. HIMACHAL PRADESH\n10. JHARKHAND\n11. KARNATKA\n12. KERLA\n13. MADHYA PARDESH\n"
				+ "14. MAHARASHTRA\n15. MANIPUR\n16. MIZORAM\n17. ODISHA\n18. PUNJAB\n19. RAJASHTHAN\n20. SIKKIM\n"
				+ "21. TAMILNADU\n22. TELANGANA\n23. TRIPURA\n24. UTTRAKHAND\n25. UTTAR PRADESH\n26. WEST BENGAL\n"
				+ "27. ANDAMAN AND NICOBAR ISLANDS\n28. CHANDIGARH\n29. DELHI\n30. JAMMU & KASHMIR\n31. LADAKH");
		System.out.println(" ");
		System.out.println("=======================================");
		System.out.println("-----Enter number to choose States-----");
		System.out.println("=======================================");
		System.out.println(" ");
		Scanner sc = new Scanner(System.in);
		int n=sc.nextInt();
		Destination d=new Destination(); // object for Destination class
		switch(n) {
		case 1: System.out.println("****************************");
			    System.out.println("WELCOME TO ANDRA PRADESH(:");
		        System.out.println("****************************");
		         d.andraPradesh();
		break;
		case 2: System.out.println("****************************");
			System.out.println("WELCOME TO ARUNACHAL PRADESH(:");
			System.out.println("****************************");
			d.arunachalPradesh();
		break;
		case 3: System.out.println("****************************");
			System.out.println("WELCOME TO ASSAM(:");
			System.out.println("****************************");
			d.assam();
		break;
		case 4: System.out.println("****************************");
			System.out.println("WELCOME TO BIHAR(:");
			System.out.println("****************************");
			d.bihar();
		break;
